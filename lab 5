
#include <iostream>
using namespace std;
int main() {
    int n;
    int arr_1[50][50];
    int arr_2[50][50];
    cout<<"Enter n: ";
    cin>>n;
    
    
    for(int i=0; i<n;i++){
        for(int j=0; j<n; j++){
            cin>>arr_1[i][j];
        }
    }
    
    for(int i=0; i<n;i++){
        for(int j=0; j<n; j++){
            cout<<arr_1[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl;
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            arr_2[i][j]=arr_1[j][i];
        }
    }
    
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cout<<arr_2[i][j]<<" ";
        }
        cout<<endl;
    }
    
    

    return 0;
}
